{{#if posts.length}}
<section>
  <h2>Your Posts</h2>
  <ul>
    {{#each posts as |post|}}
    <li>
      {{> dashboard-postinfo post}}
    </li>
    {{/each}}
  </ul>
</section>
{{/if}}

<button type="submit" class="btn"><a href="/dashboard/new">Add New Post</a></button>

<script src="/javascript/add-post.js"></script>